
{{#if hasErrors}}
    <ul class="error-list">
        {{#each errors}}
        <li>
        {{this}}
        </li>
        {{/each}}
    </ul>
    {{/if}}


<body>
  <h2>
    FAQ Section
  </h2>
  <div class="layout">

    {{#each questions}}
    <div class="accordion">
      <div class="accordion__question">
        <p>{{question}}</p>
  
      </div>
      <div class="accordion__answer">
        <p>{{answer}}</p>
      </div>
    </div>
      {{/each}}
    </div>
  </div>

      <script src="./public/js/faq.js"></script>